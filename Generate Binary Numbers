class Solution {
    public ArrayList<String> generateBinary(int n) {
        // code here
        ArrayList<String> res = new ArrayList<>();
        Queue<String> q = new LinkedList<>();
        q.add("1");
        
        for (int i = 0; i < n; i++) {
            String s = q.poll();
            res.add(s);
            q.add(s + "0");
            q.add(s + "1");
        }
        
        return res;
    }
}
