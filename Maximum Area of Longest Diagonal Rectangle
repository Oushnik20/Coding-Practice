class Solution {
    public int areaOfMaxDiagonal(int[][] dimensions) {
        int maxD = 0, maxA = 0;
        for (int[] d : dimensions) {
            int l = d[0], w = d[1];
            int diag = l * l + w * w;
            int area = l * w;
            if (diag > maxD || (diag == maxD && area > maxA)) {
                maxD = diag;a
                maxA = area;
            }
        }
        return maxA;
    }
}
