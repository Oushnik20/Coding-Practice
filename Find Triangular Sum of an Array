class Solution {
    public int triangularSum(int[] nums) {
        if(nums.length==1) return nums[0];
        int n = nums.length;
        for (int k = n - 1; k > 0; k--) {
            for (int i = 0; i < k; i++) {
                nums[i] = (nums[i] + nums[i + 1]) % 10;
            }
        }
        return nums[0];
    }
}
