class Solution {
    public int maxSubarrayXOR(int[] arr, int k) {
        // code here
        int res=0;
        int max=0;
        for(int i=0; i<k; i++) max^=arr[i];
        res=max;
        for(int i=k; i<arr.length; i++){
            max^=arr[i-k];
            max^=arr[i];
            res=Math.max(res, max);
        }
        return res;
    }
}
